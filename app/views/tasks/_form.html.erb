<%= form_with model: @task do |f| %>
      <%= f.check_box :title , class:"m-1"%>
      <%= f.label :title , class:"m-1" %>
      <%= f.text_field :title , class:"m-1" %>
      <%= f.label :details , class:"m-1" %>
      <%= f.text_field :details , class:"m-1" %>
      <%= f.label :completed , class:"m-1" %>
      <%= f.check_box :completed , class:"m-1" %>
      <%= f.submit class: 'btn btn-primary m-1' %>
<% end %>
